<?xml version="1.0" encoding="utf-8"?>
<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <general>
            <ROOT>
                <TCEforms>
                    <sheetTitle>
                        LLL:EXT:azurestorage/Resources/Private/Language/locallang.xlf:config.label.general
                    </sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <usehttps>
                        <TCEforms>
                            <label>LLL:EXT:azurestorage/Resources/Private/Language/locallang.xlf:config.label.usehttps</label>
                            <config>
                                <type>check</type>
                                <default>1</default>
                            </config>
                        </TCEforms>
                    </usehttps>
                    <containerName>
                        <TCEforms>
                            <label>LLL:EXT:azurestorage/Resources/Private/Language/locallang.xlf:config.label.container</label>
                            <config>
                                <type>input</type>
                                <size>30</size>
                                <eval>required,nospace,lower,alphanum</eval>
                                <max>63</max>
                            </config>
                        </TCEforms>
                    </containerName>
                    <accountName>
                        <TCEforms>
                            <label>LLL:EXT:azurestorage/Resources/Private/Language/locallang.xlf:config.label.accountname</label>
                            <config>
                                <type>input</type>
                                <size>30</size>
                                <eval>required,nospace,lower</eval>
                                <max>24</max>
                            </config>
                        </TCEforms>
                    </accountName>
                    <accountKey>
                        <TCEforms>
                            <label>LLL:EXT:azurestorage/Resources/Private/Language/locallang.xlf:config.label.accountkey</label>
                            <config>
                                <type>input</type>
                                <size>30</size>
                                <eval>required</eval>
                            </config>
                        </TCEforms>
                    </accountKey>
                </el>
            </ROOT>
        </general>
        <cdn>
            <ROOT>
                <TCEforms>
                    <sheetTitle>
                        LLL:EXT:azurestorage/Resources/Private/Language/locallang.xlf:config.label.cdn
                    </sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <cdnendpoint>
                        <TCEforms>
                            <label>LLL:EXT:azurestorage/Resources/Private/Language/locallang.xlf:config.label.endpoint</label>
                            <config>
                                <type>input</type>
                                <size>30</size>
                                <eval>nospace,trim</eval>
                            </config>
                        </TCEforms>
                    </cdnendpoint>
                </el>
            </ROOT>
        </cdn>
    </sheets>
</T3DataStructure>